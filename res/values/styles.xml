<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Application theme -->
    <style name="AppTheme" parent="@android:style/Theme.Holo" />


    <style name ="alert_count">
        <item name="android:paddingLeft">30dip</item>
    </style>

    <style name ="send_section">
        <item name="android:paddingRight">30dip</item>
    </style>

    <style name="EditBox">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/rectangle_text_box</item>
        <item name="android:textColor">@android:color/black</item>
        <!-- <item name="android:textCursorDrawable">@null</item> -->
        <item name="android:ems">10</item>
        <item name="android:padding">5dip</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="EditBox.Small">
        <item name="android:layout_width">100dip</item>
    </style>

    <style name="EditBox.Medium">
        <item name="android:layout_width">200dip</item>
    </style>

    <style name="EditBox.Large">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_marginLeft">10dip</item>
        <item name="android:layout_marginRight">10dip</item>
    </style>

    <style name="EditBox.Reg">
        <item name="android:layout_width">200dip</item>
        <item name="android:layout_marginTop">10dip</item>
    </style>


    <style name="main_pad_layout">
        <item name ="android:paddingLeft">10dip</item>
        <item name ="android:paddingRight">10dip</item>
        <item name ="android:paddingTop">5dip</item>
        <item name ="android:paddingBottom">5dip</item>
        <item name ="android:layout_marginLeft">15dip</item>
        <item name ="android:layout_marginRight">15dip</item>
    </style>



    <style name="main_head_marg">
        <item name="android:layout_marginLeft">30dip</item>
        <item name="android:layout_marginRight">30dip</item>
    </style>


    <style name="main_screen_top">
        <item name="android:layout_marginTop">25dip</item>
        <item name="android:layout_marginBottom">25dip</item>
    </style>


    <style name="main_under_lay">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dip</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:layout_marginLeft">30dip</item>
        <item name="android:layout_marginRight">30dip</item>
    </style>

    <style name="main_bot_lay">
        <item name ="android:layout_marginLeft">15dip</item>
        <item name ="android:layout_marginRight">15dip</item>
        <item name ="android:paddingLeft">10dip</item>
        <item name ="android:paddingRight">10dip</item>
        <item name ="android:paddingTop">5dip</item>
        <item name ="android:paddingBottom">5dip</item>
        </style>

    <style name="Button" parent="android:Widget.Holo.Button"></style>

    <style name="Button.Blue" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/btn_blue</item>
    </style>

    <style name="ButtonAppTheme" parent="android:Widget.Button">
        <item name="android:background">@drawable/btn_blue</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ImageButtonAppTheme" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/btn_blue</item>
    </style>

    <style name="EnterPin">
        <item name="android:padding">10sp</item>
    </style>

    <style name="medical_rep">
        <item name="android:layout_marginTop">50dip</item>
    </style>
    
    <style name="center_text">
        <item name="android:gravity">center_vertical|center_horizontal</item>
    </style>

    <style name="red_text">
        <item name="android:paddingTop">10dip</item>
        <item name="android:paddingLeft">10dip</item>
        <item name="android:textSize">14dip</item>
        <item name="android:textColor">#ff0000</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="step_heading">
        <item name="android:paddingTop">10dip</item>
        <item name="android:paddingLeft">10dip</item>
        <item name="android:gravity">left</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20dip</item>
    </style>

    <style name="edit_text_description">
        <item name="android:paddingLeft">10dip</item>
        <item name="android:paddingRight">10dip</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="edit_text_title">
        <item name="android:paddingTop">15dip</item>
        <item name="android:textSize">22dip</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="edit_text_title_small" parent="edit_text_title">
        <item name="android:textSize">18dip</item>
    </style>

    <style name="stock_row_top">
        <item name="android:layout_width">180dip</item>
        <item name="android:padding">10sp</item>
    </style>

    <style name="stock_row_bot">
        <item name="android:padding">10sp</item>
    </style>

    <style name="crash_error">
        <item name="android:paddingBottom">10dip</item>
        <item name="android:paddingTop">10dip</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="alert_row_sty">
        <item name="android:padding">10sp</item>
    </style>
</resources>
